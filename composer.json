{
  "name": "tejadong/graphql-mutation-validator-bundle",
  "description": "Provide a base class to validate user data sent in a GraphQL mutation and return validations errors. (Symfony 3)",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": ["GraphQL", "Validation"],
  "authors": [
    {
      "name": "AssoConnect",
      "homepage": "https://www.assoconnect.com"
    },
    {
      "name": "Sylvain Fabre (author owner)"
    },
    {
      "name": "Jesus Tejada (author modifier)"
    }
  ],
  "autoload": {
    "psr-4": {
      "tejadong\\GraphQLMutationValidatorBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": "^5.6",
    "overblog/graphql-bundle": "^0.11",
    "symfony/validator": "3.4.17",
    "doctrine/annotations": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "symfony/twig-bundle": "3.4.17",
    "symfony/security-bundle": "3.4.17",
    "symfony/yaml": "3.4.17",
    "symfony/templating": "3.4.17",
    "symfony/console": "^3.4.17"
  }
}
